{
    "name": "storecampCore/activity-logger",
    "description": "Keep a change history for your models using laravel version 5.*",
    "keywords": ["log", "activity", "auditing", "laravel", "register", "observe"],
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Syrinx Web",
            "email": "ivan@combird.nl"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "~5.0|~5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\Core\\Auditing\\": "src/"
        }
    },    
    "autoload-dev": {
	"classmap": ["tests"]
    },
    "minimum-stability": "dev"
}
